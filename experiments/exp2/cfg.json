{
  "device": "cuda:0",
  "MAX_EPOCHES": 80,
  "model": "enet_sad",

  "dataset": {
    "dataset_name": "CULane",
    "batch_size": 12,
    "resize_shape": [800, 288]
  },

  "optim": {
    "lr": 1e-2,
    "momentum": 0.9,
    "weight_decay": 2e-5,
    "nesterov": true
  },

  "lr_scheduler": {
    "warmup": 50,
    "max_iter": 60000
  },

  "sad_start_iter": 40000
}